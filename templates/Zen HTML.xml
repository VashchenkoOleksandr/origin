      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="all" value="{{#all $COND$}}&#10;    $END$&#10;{{/all}}" shortcut="TAB" description="BigCommerce ALL" toReformat="false" toShortenFQNames="true">
