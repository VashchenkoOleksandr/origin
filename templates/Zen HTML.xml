    <variable name="STATE1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="for" value="{{#for $NUM1$ $NUM2$}}&#10;    $END$&#10;{{/for}}" shortcut="TAB" description="BigCommerce for" toReformat="false" toShortenFQNames="true">
